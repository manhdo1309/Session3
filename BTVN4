#include <stdio.h>
int main(){
	int number1, number2, number3;
	printf("Nhap vao 3 so nguyen: ");
	scanf("%d %d %d", &number1, &number2, &number3);
	int min = number1;
	int max = number1;
	if(max < number2)
	{
		max = number2;
	}
	if(max < number3)
	{
		max = number3;
	}
	if(min > number2)
	{
		min = number2;
	}
	if(min > number3)
	{
		min = number3;
	}
	printf("So lon nhat la: %d\n", max);
	printf("So nho nhat la: %d", min);
}
